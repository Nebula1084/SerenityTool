.ORIGIN 0x4100
		push	$a0				#$a0, character that would be printed
		push	$a1				#$a1, x
		push	$a2				#$a2, y
		push	$t0
		addi	$t0, $zero, 40  # width of the screen
		mul		$a1, $a1, $t0
		add		$a1, $a1, $a2
		addi	$a1, $a1, 0x3000
		sh		$a0, 0($a1)
		
		pop		$t0
		pop		$a2
		pop		$a1
		pop		$a0
		jr		$ra
		
