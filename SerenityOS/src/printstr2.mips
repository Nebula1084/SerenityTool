.ORIGIN		0x4380
			push	$a0, $a1, $t0, $t1		
			move	$t1, $zero
			
LOOP:		lh		$t0, 0($a0)
			beq		$t0, $zero, EXIT
			sh		$t0, 0x3000($a1)
			addi	$a0, $a0, 1
			addi	$a1, $a1, 1
			addi	$t1, $t1, 1
			j		LOOP
			
EXIT:		li		$t0, 20
LOOP2:		sh		$zero,0x3000($a1)
			addi	$a1, $a1, 1
			addi	$t1, $t1, 1
			bne		$t0, $t1, LOOP2
			
			pop		$a0, $a1, $t0, $t1
			jr		$ra	