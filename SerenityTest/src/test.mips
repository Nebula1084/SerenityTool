		addi	$sp, $zero, 0x2FFE #initial the stack pointer
		
		addi 	$t0, $zero, 2
		sh 		$zero, 0x5001($zero)
		sh 		$t0, 0x5002($zero)
sus:	lh 		$t1, 0x5003($zero)
		beq		$t1, $zero, sus
		addi	$a0, $zero, 0x5100
		add 	$a1, $zero, $zero
		addi	$v0, $zero, 1
		syscall
		
exit:	beq		$zero, $zero, exit

